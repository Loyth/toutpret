<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="toutpret.isep.com.toutpret">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".login_sinup.LoginActivity" />
        <activity android:name=".login_sinup.SignUpActivity" />
        <activity android:name=".categories.CategoriesActivity" />
        <activity android:name=".products.ProductsActivity" />
        <activity android:name=".orderpicker.OrderPickerActivity" />
        <activity android:name=".orderpicker.OrderPicker_Catalogue" />
        <activity android:name=".orderpicker.OrderPicker_Commandes" />
        <activity android:name=".commandes.CommandesActivity" />
        <activity android:name=".Profil.ProfilActivity"></activity>
    </application>

</manifest>